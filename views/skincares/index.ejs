<!--add skincare product info-->
<!DOCTYPE html>
<html lang="en">
<%- include('../partials/head') %>

<body>
  <%- include('../partials/header') %>
  <main class="container">
    <h1>Select a Skincare Product</h1>
    <p>Brand Selector</p>
    <div>
        <% let listOfBrands = [] %>
        <% skincares.forEach(function(s) { %>
            <% if (!listOfBrands.includes(s.brandName)) { %>
                <% listOfBrands.push(s.brandName) %>
            <% } %>
            <a href="/skincares/<%= skincare._id%>">Testing</a>"
        <% }); %>
           
    </div>
    <p>You have entered your skincare product into your beauty box <%= skincare.brandName %>!</p>
    
  </main>
  <%- include('../partials/footer') %> 
</body>
</html>